{
  "name":"ShopifyConnector",
  "description":"Shopify connector for WhiteRabbit suite",
  "icon":"fa-shopping-basket",
  "install":"internal",
  "channels":[
    {
      "name":"Shopify",
      "default_channel":"1",
      "streams":[
        {
          "name":"Products",
          "permitted_operations":"RPD",
          "connection_string":"ShopifyProductConnector",
          "type": "product"
        }
      ],
      "settings":[
        {
          "name":"access_token",
          "type":"string",
          "is_editable":"1",
          "is_mandatory":"1",
          "access_level":"admin"
        },
        {
          "name":"scope",
          "type":"string",
          "is_editable":"1",
          "is_mandatory":"1",
          "access_level":"admin"
        }
      ]
    }
  ]
}